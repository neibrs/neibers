{#
/**
 * @file cabinet.html.twig
 * Default theme implementation to present Cabinet data.
 *
 * This template is used when viewing Cabinet pages.
 *
 *
 * Available variables:
 * - content: A list of content items. Use 'content' to print all content, or
 * - attributes: HTML attributes for the container element.
 *
 * @see template_preprocess_cabinet()
 *
 * @ingroup themeable
 */
#}
<div{{ attributes.addClass('cabinet') }}>
  {% if content %}
    {{- content -}}
  {% endif %}
</div>
